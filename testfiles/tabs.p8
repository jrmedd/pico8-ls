-- first tab

function _init()
	elapsed_time = 0
end

function _update()
	elapsed_time += 1
end

function _draw()
	draw_tab = flr(elapsed_time / 10) % 5
	print(draw_tab)
	if draw_tab == 0 then
		draw_0()
	elseif draw_tab == 1 then
		draw_1()
	elseif draw_tab == 2 then
		draw_2()
	elseif draw_tab == 3 then
		draw_3()
	elseif draw_tab == 4 then
		draw_4()
	end
end

function draw_0()
	cls(0)
	print("tab 0", 54, 61, elapsed_time)
end
